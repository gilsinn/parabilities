<div class="navbar navbar-expand-sm navbar-light navbar-test">
  <div class="container-fluid">

    <i class="fa-solid fa-house">
      <%= link_to "", root_path, class: "nav-link"  %>
    </i>

    <i class="fa-solid fa-heart">
      <%= link_to "", user_favourites_path(current_user), class: "nav-link"  %>
    </i>

    <i class="fa-solid fa-house-chimney-user">
      <%= link_to "", community_path, class: "nav-link"  %>
    </i>

    <% if user_signed_in? %>
      <div class="dropup">
        <%= cl_image_tag 'albert-dera-ILip77SbmOE-unsplash_yfzrit', class:"avatar", width: 100, height: 100, crop: :fill, gravity: :face %>
        <div class="dropup-content">
          <%= link_to "Action", "#", class: "dropdown-item" %>
          <%= link_to "Another action", "#", class: "dropdown-item" %>
          <%= link_to "Log out", destroy_user_session_path, method: :delete, class: "dropdown-item" %>
        </div>
      </div>
    <% else %>
      <li class="nav-item">
        <i class="fa-solid fa-user"><%= link_to "", new_user_session_path, class: "nav-link" %></i>
      </li>
    <% end %>

    <%# <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
